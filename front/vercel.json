{
    "rewrites": [
        {
            "source": "/.well-known/:path*",
            "destination": "/.well-known/:path*"
        }
    ],
    "headers": [
        {
            "source": "/.well-known/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/json"
                },
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                }
            ]
        }
    ]
}